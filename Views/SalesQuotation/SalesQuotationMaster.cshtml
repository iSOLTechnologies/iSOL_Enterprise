    function ValidateData(Id) {

        if ($("#" + Id).val() == null || $("#" + Id).val() == '' || $("#" + Id).val() == undefined) {
            $("#" + Id).addClass('is-invalid');
            return false;
        }
        else {
            $("#" + Id).removeClass('is-invalid');
            return true;
        }

