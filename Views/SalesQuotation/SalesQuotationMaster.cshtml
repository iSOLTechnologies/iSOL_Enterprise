            var MinWidth = $(this).closest('#ListParameters .itm').find("#MinWidth");
            var MaxWidth = $(this).closest('#ListParameters .itm').find("#MaxWidth");

        });

        for (var i = 0; i < array.length; i++) {
            if (array[i] == false) {
                isValid = false;
            }
        }



        if (isValid != false) {
            if (Guid != undefined && Guid != '' && Guid != null && isDuplicate == false) {
