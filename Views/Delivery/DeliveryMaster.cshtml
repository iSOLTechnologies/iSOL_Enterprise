    function getJsonObj(element) {

        var jsonobj = new Object();

        // console.log($('#' + element + ' input,' + '#' + element + ' select'))

        $('#' + element + ' input,' + '#' + element + ' select').each(function (index, data) {

            if ($(this).attr("id") != undefined || $(this).attr("id") != "" || $(this).attr("id") != '')
                jsonobj[$(this).attr("id")] = $(this).val();

        });
        return jsonobj;
    }

    function getJsonObjList(element) {
        let listObj = [];

        $('#' + element).each(function (index, item) {
            var jsonobj = new Object();
            $(this).find('input , select').each(function (index, data) {

                if ($(this).attr("id") != undefined || $(this).attr("id") != "" || $(this).attr("id") != '')
                    jsonobj[$(this).attr("id")] = $(this).val();

            });

            listObj.push(jsonobj);



        })
        return listObj;
    }
