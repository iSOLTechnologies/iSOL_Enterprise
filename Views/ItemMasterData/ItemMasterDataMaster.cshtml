        $(document).on("input", "input[type='number']", function () {
            var value = parseFloat(this.value);
            if (isNaN(value)) {
                return;
            }
            var decimalCount = (this.value.split(".")[1] || []).length;
            if (decimalCount > 3) {
                this.value = value.toFixed(3);
            }
