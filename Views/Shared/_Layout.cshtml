    <script>
        function GetAttendance() {


            $.ajax({
                url: "/Attendance/GetData",//_Administrator/GetRoleDtl
                type: "Get",
                data: { UserId: $('#_UserId').val()},
                dataType: 'json',
                success: function (resp) {
                    return resp;
                },
                error: function (jqXhr, textStatus, errorMessage) {

                }
            }).done(function (result) {
                console.log(result.data);
                LoadAttendanceCalender(result.data);
            });
        }
        function LoadAttendanceCalender(data) {
           var attArray = [];
            
            for (var i = 0; i < data.length; i++) {
                if (data[i].msg == 'Late In') {

                    var formdata = {
                        title: "Late Im", 
                        start: moment(data[i].datetime).format("YYYY-MM-DD"),
                        description: "Late In " + moment(data[i].in).format(" hh:mm:ss "),
                        className:"fc-event-danger fc-event-solid-warning"
                    };
                }
                if (data[i].msg == 'Present') {
                    var formdata = {
                        title: "Present",
                        start: moment(data[i].datetime).format("YYYY-MM-DD"),
                        description: "Present " + moment(data[i].in).format(" hh:mm:ss "),
                        className: "fc-event-light fc-event-solid-primary"
                    };
                }
               
                attArray.push(formdata);
            }
            console.log(attArray);
            
            var todayDate = moment().startOf('day');
            var YM = todayDate.format('YYYY-MM');
            var asd = YM + '-01';
            var YESTERDAY = todayDate.clone().subtract(1, 'day').format('YYYY-MM-DD');
            var TODAY = todayDate.format('YYYY-MM-DD');
            var TOMORROW = todayDate.clone().add(1, 'day').format('YYYY-MM-DD');

            var calendarEl = document.getElementById('kt_calendar');
            var calendar = new FullCalendar.Calendar(calendarEl, {
                plugins: ['interaction', 'dayGrid', 'timeGrid', 'list'],

                isRTL: KTUtil.isRTL(),
                header: {
