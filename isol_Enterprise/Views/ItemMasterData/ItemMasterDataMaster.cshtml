        const boxes = document.querySelectorAll('.d');

        for (const box of boxes) {
            box.value = new Date().toJSON().slice(0, 10);
        }

        
        $(document).on("input", "input[type='number']", function () {
            var value = parseFloat(this.value);
            if (isNaN(value)) {
                return;
            }
            var decimalCount = (this.value.split(".")[1] || []).length;
            if (decimalCount > 3) {
                this.value = value.toFixed(3);
            }

            if (this.value.length > 10) {
                this.value = this.value.slice(0, 10);
            }
        });

        $(document).on('click', '#PropertiesSelectAll', function (e) {

                $("#Properties tr").each(function () {
                    $(this).find(":checkbox").prop("checked", true);
                });
        });
        
        $(document).on('click', '#PropertiesClearAll', function (e) {

                    $("#Properties tr").each(function () {
            $(this).find(":checkbox").prop("checked", false);

                });
        }); 
        
        $(document).on('change', '#ManBtchNum', function (e) {
                    let ManBtchNum = $('#ManBtchNum').val();
                    if(ManBtchNum == "N" || ManBtchNum == "Y"){
                        $('#MyMngMthd').removeClass('d-none');
                    }else{
                        $('#MyMngMthd').addClass('d-none');
                    }        
        });

        $(document).on('click', '#ActivevalidFor,#InActivevalidFor', function (e) {
        //console.log("$('input[name=validFor]:checked').val()",$('input[name=validFor]:checked').val());
        if ($('input[name=validFor]:checked').val() === 'Y') {
            $('#ActivevalidFor1').removeClass('d-none');
            $('#InActivevalidFor1').addClass('d-none');

            }else if($('input[name=validFor]:checked').val() === 'N') {
            $('#ActivevalidFor1').addClass('d-none');
            $('#InActivevalidFor1').removeClass('d-none');

            }
        });        
        
        $(document).on('click', '#btnBack', function (e) {
            window.history.back();
        });

        $(document).on('click', '#ListBtnDelete', function () {
            $(this).closest('tr').remove();
        });
        
        $(document).on('change', '#MySeries', function () {


        if ($(this).val() == -1) {

            $('#ItemCode').attr("readonly", false);
            $('#ItemCode').val("");
        }
        else {
            $('#ItemCode').attr("readonly", true);

            return $.ajax({
                url: '/ItemMasterData/GetNewItemCode',
                dataType: 'json',
                type: 'Get',

                data: { Series: $(this).val() },

                success: function (result) {

                    if (result.success) {

                        $('#ItemCode').val(result.itemCode);

                    }

                    if (!(result.success)) {
                        toastr.error("An error occured while getting ItemCode");

                    }

                },
                error: function (jqXhr, textStatus, errorMessage) {
                    console.log(errorMessage);
                }

            }).done(function (result) {

            });
        }
    })
  
    </script>
