<script>
    $(document).ready(function() {
             
            
             
             $("#CstGrpCode1").val($('#CstGrpCode option:selected').val());  
             $("#VatGroupSa1").val($('#VatGroupSa option:selected').attr("guid"));
             $("#VatGroupPu1").val($('#VatGroupPu option:selected').attr("guid"));


            $("#BuyUnitMsr").select2({
            placeholder: "Select an option",
            tags: true,
            tokenSeparators: [',', ' ']
            });
            
            $("#SalUnitMsr").select2({
            placeholder: "Select an option",
            tags: true,
            tokenSeparators: [',', ' ']
            });
            $("#InvntryUom").select2({
            placeholder: "Select an option",
            tags: true,
            tokenSeparators: [',', ' ']
            });

            $("#VatGroupPu").select2();

            if (@ViewBag.OldItemId != 0) 
            {
                OldItemId = @ViewBag.OldItemId;
                isEdit = true;
                
                $("#btnSaveData").html('<i class="ki ki-check icon-sm"></i> Update');
                $("#btnSaveData").attr('disabled', true);
                $("#MySeries").attr('disabled', true);
                $("#Series").attr('disabled', true);
                $("#ItemCode").attr('disabled', true);
                GetOldData();
                
            }
    });

    $(document).on("keydown", "input", function () {
        $(this).removeClass('is-invalid');
    });
    
    $(document).on('change' ,'#InvntItem' , function(){
        
            if( $(this).is(':checked'))
            {
                $("#InvntryUom").removeClass("NotReq");
                isInventryItem = true;
            }
            else
            {
                $("#InvntryUom").addClass("NotReq");
                isInventryItem = false;
            }
    });
    
    $(document).on('change' ,'#SellItem' , function(){
         
            if( $(this).is(':checked') )
            {
                $("#SalUnitMsr").removeClass("NotReq");
                isSalesItem =  true;
            }
            else
            {
                $("#SalUnitMsr").addClass("NotReq");
                isSalesItem =  false;
            }

    });
    
    $(document).on('change' ,'#PrchseItem' , function(){
        
            if( $(this).is(':checked') )
            {
                $("#BuyUnitMsr").removeClass("NotReq");
                isPurchaseItem = true;
            }
            else 
            {
                $("#BuyUnitMsr").addClass("NotReq");
                isPurchaseItem = false;
            }
    });
    
    $(document).on('change' ,'#ByWh' , function(){

            if( $(this).is(':checked') )
            {
                $("#kt_tabs_inventoryData_Warehouses").removeClass("d-none");
                isManageInvByWareHouse = true;
            }
            else 
            {
                $("#kt_tabs_inventoryData_Warehouses").addClass("d-none");
                isManageInvByWareHouse = false;
            }
    })

    $(document).on('change', '#CstGrpCode', function (e) {

        $("#CstGrpCode1").val($(this).val());  
                    
    });    

    $(document).on('change', '#VatGroupSa', function (e) {

        $("#VatGroupSa1").val($('#VatGroupSa option:selected').attr("guid"));
                          
                    
    });         

    $(document).on('change', '#VatGroupPu', function (e) {
        

        $("#VatGroupPu1").val($('#VatGroupPu option:selected').attr("guid"));

    });

    $(document).on('click', '#btnSaveData', function (e) {

       // console.log(getJsonObjList("ListParameters tr"));

        var isValid = false;
        
        $(this).addClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
        $(this).attr('disabled', true);
        if ( ValidateData("HeaderForm") ) 
        {
             
            
            if (isPurchaseItem) 
            {
                  isValid =  ValidateData("kt_tabs_PurchasingData");
            }
            if (isSalesItem) 
            {
                  isValid =  ValidateData("kt_tabs_SalesData");
            }
            if (isInventryItem) 
            {
                  isValid =  ValidateData("kt_tabs_InventoryData");
            }
            if (isInventryItem && isManageInvByWareHouse) 
            {
                  isValid =  ValidateListData("ListParameters itm");
            }    
            if(isValid) 
            {
                $.when( 
             
                    isValid = ValidateData("kt_tabs_PlanningData"),
                    isValid = ValidateData("kt_tabs_ProductionData"),
                

                ).then(function() 
                {
                    if(isValid)
                    FormData();
                    else
                    {
                        $("#btnSaveData").removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
                        $("#btnSaveData").attr('disabled', false);
                        toastr.warning("Validations Error");
                    }
                })
            }
            else {
                $("#btnSaveData").removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
                $("#btnSaveData").attr('disabled', false);
                toastr.warning("Validations Error");
            }
        }
        else 
        {
            $(this).removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
            $(this).attr('disabled', false);
        }

    });

    function FormData() 
    {
            
        var data =
        {
            HeaderData: getJsonObj("HeaderForm"),
            
            Tab_General : getJsonObj("kt_tabs_General"),
            Tab_PurchasingData : isPurchaseItem == true ? getJsonObj("kt_tabs_PurchasingData") : null, 
            Tab_SalesData :  isSalesItem == true ? getJsonObj("kt_tabs_SalesData") : null,
            Tab_InventoryData : isInventryItem == true ? getJsonObj("kt_tabs_InventoryData") : null,
            Tab_InventoryData_WareHouseList: isInventryItem == true && isManageInvByWareHouse == true ? getJsonObjList("ListParameters tr") : null,
            Tab_PlanningData : getJsonObj("kt_tabs_PlanningData"),
            Tab_ProductionData : getJsonObj("kt_tabs_ProductionData"),
            Tab_Properties : getJsonObj("kt_tabs_Properties"),
            OldItemId: OldItemId
                  
        }
        
       
       PostData(data);

    
    }

    function PostData(formData) 
    {
      
        
      
        return $.ajax({
            url: '/ItemMasterData/AddItemMasterData',
            dataType: 'json',
            type: 'POST',
            
             data: { formData: JSON.stringify(formData)},

            success: function (result) 
            {
                console.log(result);
                if (result.isInserted) {
                    toastr.success(result.message);

                    setTimeout(
                        function () 
                        {
                            $('#btnSaveData').removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
                            $('#btnSaveData').attr('disabled', false);
                        }, 800);
                    setTimeout(function () {
                        window.history.back();
                    }, 800);


                }

                if (!(result.isInserted)) {
                    toastr.error(result.message);
                    $('#btnSaveData').removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
                    $('#btnSaveData').attr('disabled', false);

                }

            },
            error: function (jqXhr, textStatus, errorMessage) {
                console.log(errorMessage);
            }

        }).done(function (result) {

        });
    }



    function GetOldData() 
    {

        $.ajax({
            url: 'ItemMasterData/GetItemOldData',
            type: 'GET',
            dataType: 'json',
            async : false,
            data: { ItemID: OldItemId },
            success: function (data) 
            {
                
                let OITMData = JSON.parse(data.oitmData);  
                
                $.each(OITMData[0][0], function (key, value) {
                   
                    if ($("#" + key).is(':checkbox') || $(this).is(':radio'))
                    {
                         if(value == 'Y') {
                         $("#" + key).prop('checked', true);
                         
                         }
                         else
                         {
                           
                            $("#" + key).prop('checked', false);
                            
                         }
                    }
                    else if($("#" + key).attr('type') === 'date') 
                    {
                        $("#" + key).val(moment(value).format('yyyy-MM-DD'));
                        
                    }
                    else
                    {
                         $("#" + key).val(value);
                    }

                });
               //Add Warehouse Data
                let j = 0;
                $.each((data.whsData), function () 
                {
                    if(j == 0) 
                    {
                        
                        $("#WhsCode").val(this.whscode);

                        $("#WhsName").val(this.whsname);

                        if(this.locked == 'Y')
                        $("#Locked").prop('checked', true);
                        else
                        $("#Locked").prop('checked', false);

                        $("#MinStock").val(this.minStock);

                        $("#MaxStock").val(this.maxStock);

                        $("#MinOrder").val(this.minOrder);

                        if (!(this.isEditable)) {
                            $("#WhsCode").attr('disabled', true);
                            $("#WhsName").attr('disabled', true);
                            $("#Locked").attr('disabled', true);
                            $("#MinStock").attr('disabled', true);
                            $("#MaxStock").attr('disabled', true);
                            $("#MinOrder").attr('disabled', true);
                            $("#ByWh").attr('disabled', true);
                       }

                    }
                    else{
                        AddWarehouseRowData(this);
                    }
                    j+=1;
                    

                });
                //EditTable
                
                
                if (!(data.isEditable.value.purchaseItem)) {
                    $("#PrchseItem").attr('disabled', true);
                    $("#ItmsGrpCod").attr('disabled', true);
                }
                if (!(data.isEditable.value.salesItem)) {
                    $("#SellItem").attr('disabled', true);
                    $("#ItmsGrpCod").attr('disabled', true);
                }
                if (!(data.isEditable.value.inventoryItem)) {
                    $("#InvntItem").attr('disabled', true);
                    $("#ItmsGrpCod").attr('disabled', true);
                }
                
                let PrchseItem = document.getElementById("PrchseItem");
                let SellItem = document.getElementById("SellItem");
                let InvntItem = document.getElementById("InvntItem");

                var event = new Event("change", { bubbles: true });
                PrchseItem.dispatchEvent(event);
                SellItem.dispatchEvent(event);
                InvntItem.dispatchEvent(event);

                $("#btnSaveData").attr('disabled', false);
                
                
            },
            error: function (jqXhr, textStatus, errorMessage) {
                console.log(errorMessage);
            }
        });
    }
</script>
