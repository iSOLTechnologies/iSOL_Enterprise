                                                <label class="checkbox checkbox-outline checkbox-primary">
                                                <input type="checkbox" name="CfwRlvnt" />
                                                    <span></span>
                                                    Cash Flow Relevant
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div style="font-size:1rem" class="font-weight-bold"> Relevant for Cost Accounting </div>

                                <div class="container pt-5">
                                    <div class="form-group row">
                                        <div class="col-md-4 pt-3">
                                            <div class="checkbox-inline">
                                                <label class="checkbox checkbox-outline checkbox-primary">
                                                    <input type="checkbox" name="PrjRelvnt" id="PrjRelvnt" />
                                                    <span></span>
                                                    Project
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-md-8 d-flex">
                                        
                                            <div class="col-lg-12 col-xl-12">
                                            <select name="Project" id="Project" class="form-control NotReq" disabled>

                                                
                                                @foreach (var pro in ViewBag.Projects)
                                                {
                                                    <option value="@pro.Value">@pro.Text</option>
                                                }
                                            </select>
                                                

                                            </div>
                                        </div>
                                    </div>
                               </div>
                        </div>
                   </div>
               
                    <div class="col-md-6">
                    
                        <div class="col-md-12 col-sm-12 scroll-smooth scrollbar Tree_container text-sm" id="">
                            <div class="row justify-content-center  mb-2" id="DrawerHeading">
                                <span class="font-weight-bold" style="font-size:1.4rem"> Assets </span>
                            </div>
                            <div class="row justify-content-center" id="ButtonsContainer">

                                <a class="btn btn-text-warning btn-hover-light-warning font-weight-bold btn-sm" id="btnExpColl"> <i class="fas fa-minus text-warning icon-nm"></i>Collapse All</a>
                                <a class="btn btn-text-primary btn-hover-light-primary font-weight-bold btn-sm" id="btnSelUnSel"> <i class="fas fa-times text-primary icon-nm"></i> UnSelect All</a>

                            </div>
                            <div id="tree">
                        
                            </div>


                        </div>
                    </div>
                </div>
                
                <div class="modal fade" id="AccountDetailModel" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
