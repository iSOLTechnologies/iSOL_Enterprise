<script src="~/Assets/plugins/custom/prismjs/prismjs.bundle.js"></script>
<script src="~/Assets/js/scripts.bundle.js"></script>
<script src="~/js/myjson_validate.js"></script>

<script>
    var tree = "";
    var AccountDetail = null;

    var treeDataSource = '/ChartOfAccounts/GetLevels';
    $(document).ready(function () {

        let Guid = $('#Guid').val();

        if (Guid != null && Guid != '' && Guid != undefined) 
        {
            $("#btnSaveData").html('<i class="ki ki-check icon-sm"></i> Update');
            $("#btnSaveData").attr('disabled', true);
            $('input[name=drawers]').attr('disabled', true);
            $("#HeaderTitle2").text('Update');
            GetGLAccountData(Guid);
        }
        else 
        {
        
            GetLevels($("input[name=drawers]:checked").val());
            treeDataSource = '/ChartOfAccounts/GetLevels';
            GetUpdatedAcctCode(1);
            $("#FatherNum").val(1);
            UpdateLevel(1);
        
        }
    });

    $(document).on('change', "#PrjRelvnt", function () {

        if ( $(this).is(':checked') )
            $("#Project").attr('disabled',false);
        else
            $("#Project").attr('disabled',true);

    });
    
    $(document).on('change', "input[name=postable]", function () {

        let Postable = $("input[name=postable]:checked").val();
        if(Postable === "Y") 
        {
            $("#FooterForm").removeClass('d-none');
        }
        else
        {
            $("#FooterForm").addClass('d-none');
        }

    });

    $(document).on('click', ".list-group-item", function () {

        let SelectedLevelTag = $('li.list-group-item.active');
        
        let FatherNum = $(SelectedLevelTag).attr('data-id');
        let TargetSpan = $('li[data-id="' + FatherNum + '"] span.act:first');

        let Level = $(TargetSpan).attr('act-level');
        let Postable = $(TargetSpan).attr('act-postable');
        
        
        

        if ($("#AcctCode").val() != FatherNum)
        {
            $("#FatherNum").val(FatherNum);
            UpdateLevel(Level);

            if (Postable === "Y") {
                UpdateFormsAsActiveAccount();

            }
            else {
                UpdateFormAsTitleAccount();
            }
            if ($('#Guid').val() == "" || $('#Guid').val() == null)
                GetUpdatedAcctCode(FatherNum);
        }
        else
        {
            toastr.warning("Select different account !");
            $('li.list-group-item.active').removeClass("active");
        }
    });
    
    $('input[name=drawers]').change(function () {
        
        $('#tree').tree().destroy();
        
        var selectedValue = $(this).val();
        
        $("#DrawerHeading span").text($(this).attr('placeholder'));
        UpdateLevel(1);
        GetLevels(selectedValue);
        UpdateFormsAsActiveAccount();        
        $("#FatherNum").val(selectedValue);
        GetUpdatedAcctCode(selectedValue);
        UpdateAccountType(selectedValue);

    });

    function UpdateFormsAsActiveAccount() 
    {
        $("#ActiveAccount").attr('disabled', false);
        $("#ActiveAccount").prop('checked', true);
        $("#FooterForm").removeClass('d-none');
        $("#TitleAccount").attr('disabled', false);
    }
    
    function UpdateFormAsTitleAccount() {
        
        $("#ActiveAccount").attr('disabled', true);
        $("#ActiveAccount").prop('checked', false);
        $("#FooterForm").addClass('d-none');
        $("#TitleAccount").attr('disabled', true);
   }
   
    function UpdateLevel(value) {

        value = parseInt(value, 10);
        

        $.when($("#Levels").val(value)).then(function() {
            RemoveDecimal("Levels");
        })
        
    }
    
    function RemoveDecimal(element) 
    {
        
        var inputElement = $('#' + element);
        
        if (inputElement.val().includes('.')) {
            
            inputElement.val(parseInt(inputElement.val()));
        }
    }
    
    $('#Levels').on('input', function () {
        
        RemoveDecimal("Levels");
    });
    function UpdateAccountType(drawer) {

        if(drawer === "1" || drawer === "2" || drawer === "3")
            $("#ActType").val("N")
        else if(drawer === "4")
            $("#ActType").val("I")
        else
            $("#ActType").val("E")
    }
    
    $(document).on('click', 'input[name=validFor]', function (e) {
        
        if ($('input[name=validFor]:checked').val() === 'Y') 
        {
            $('#ActivevalidFor1').removeClass('d-none');
            $('#InActivevalidFor1').addClass('d-none');

        } else if ($('input[name=validFor]:checked').val() === 'N') 
        {
            $('#ActivevalidFor1').addClass('d-none');
            $('#InActivevalidFor1').removeClass('d-none');

        }
    });
   
    function GetUpdatedAcctCode(FatherNum) 
    {

        return $.ajax({
            url: '/ChartOfAccounts/GetUpdatedAcctCode',
            dataType: 'json',
            aysnc: false,
            data: { FatherNum: FatherNum },
            type: 'Post',
            success: function (result) {

            },
            error: function (jqXhr, textStatus, errorMessage) {
                console.log(errorMessage);
            }


        }).done(function (result) {
            
            $("#AcctCode").val(result);
        });

    }

    $(document).on('click', "#btnSelUnSel", function () {

        if ($(this).html() == '<i class="fas fa-check text-primary icon-nm"></i> Sellect All') {
            tree.checkAll();
            $(this).html('<i class="fas fa-times text-primary icon-nm"></i> UnSelect All');
        }
        else {
            tree.uncheckAll();
            $(this).html('<i class="fas fa-check text-primary icon-nm"></i> Sellect All');
        }
    });
    
    $(document).on('click', "#btnExpColl", function () {

        if ($(this).html() == '<i class="fas fa-minus text-warning icon-nm"></i>Collapse All') {
            tree.collapseAll();
            $(this).html('<i class="fas fa-plus text-warning icon-nm"></i>Expand All');
        }
        else {
            tree.expandAll();
            $(this).html('<i class="fas fa-minus text-warning icon-nm"></i>Collapse All');
        }
    });

    function GetLevels(drawer) {

        tree = $('#tree').tree({
            primaryKey: 'id',
            uiLibrary: 'bootstrap4',
            dataSource: treeDataSource + "?drawer='"+drawer+"'",
            checkboxes: false,
            
            //cascadeCheck: false,
        });

        tree.on('dataBound', function () {
            tree.expandAll();
        });

    }

    $(document).on('click', '#btnAccoutDetails', function (e) {

        $('#AccountDetailModel').modal('show');
    });

    $(document).on('click', '#btnAccountDetailOk', function (e) {

        AccountDetail = getJsonObjKeyName("AccountDetail");
        $('#AccountDetailModel').modal('hide');
    });

    $(document).on('click', '#btnSaveData', function (e) {



        $(this).addClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
        $(this).attr('disabled', true);

        if (ValidateData("HeaderForm")) {

            FormData();
        }
        else {

            $(this).removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
            $(this).attr('disabled', false);
        }
    });

   
    function FormData() {
        
        var formdata =
        {

            HeaderData: getJsonObjKeyName("HeaderForm"),
            FooterData: getJsonObjKeyName("FooterForm"),
            AccountDetail: AccountDetail
            
        }

        

        PostData(formdata);


    }

    function PostData(formData) {

        let Guid = $('#Guid').val();
        let URL = '/ChartOfAccounts/Add';
        if (Guid != null && Guid != '' && Guid != undefined) {
            URL = '/ChartOfAccounts/Edit';
        }

        return $.ajax({
            url: URL,
            dataType: 'json',
            type: 'POST',

            data: { formData: JSON.stringify(formData) },

            success: function (result) {
                console.log(result);
                if (result.isInserted) {
                    toastr.success(result.message);

                    setTimeout(
                        function () {

                            $('#btnSaveData').removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
                            $('#btnSaveData').attr('disabled', false);

                        }, 800);
                    setTimeout(function () {
                        window.history.back();
                    }, 800);


                }

                if (!(result.isInserted)) {
                    toastr.error(result.message);
                    $('#btnSaveData').removeClass('kt-spinner kt-spinner--right kt-spinner--md kt-spinner--light');
                    $('#btnSaveData').attr('disabled', false);

                }

            },
            error: function (jqXhr, textStatus, errorMessage) {
                console.log(errorMessage);
            }

        }).done(function (result) {

        });
    }

    function GetGLAccountData(Guid) {
        $.ajax({
            url: 'ChartOfAccounts/GetGlAccountData',
            type: 'GET',
            dataType: 'json',
            async: false,
            data: { Guid: Guid },
            success: function (data) 
            {
                let GLAcctData = JSON.parse(data.gLAcctData);
                let Drawer = JSON.parse(data.drawer);

                $('input[name=drawers][value="' + Drawer + '"]').prop('checked', true);

                console.log(GLAcctData);

                $.each(GLAcctData[0][0], function (key, value) {

                    if ($('[name="'+key+'"]').is(':checkbox') ) {
                        if (value == 'Y') {
                            $('[name="'+key+'"]').prop('checked', true);

                        }
                        else {

                            $('[name="'+key+'"]').prop('checked', false);

                        }
                    }
                    else if ($('[name="' + key + '"]').is(':radio')) 
                    {
                        $('[name="'+key+'"][value="'+value+'"]').prop('checked', true);
                    }
                    else if ($('[name="'+key+'"]').attr('type') === 'date') {
                        $('[name="'+key+'"]').val(moment(value).format('yyyy-MM-DD'));

                    }
                    else 
                    {
                        if(key == "Levels")
                            UpdateLevel(value - 1);
                        else
                        $('[name="'+key+'"]').val(value);
                    }

                });
            
                GetLevels(Drawer);
                AccountDetail = getJsonObjKeyName("AccountDetail");
                $("#btnSaveData").attr('disabled', false);
            } 
        });

    }
</script>










